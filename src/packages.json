{
    "basePath": "uibase/",
    
    "jQuery": {
        "scripts": "commons/external/jquery.js"
    },

    "baseUtils": {
        "scripts": "commons/utils/utils.js"
    },

    "utils": {
        "dependencies": ["baseUtils"],
        "scripts": [
            "commons/utils/class.js",
            "commons/utils/contracts.js"
        ]
    },

    "observer": {
        "dependencies": ["utils"],
        "scripts": "commons/observer.js"
    },

    "observable": {
        "dependencies": ["observer"],
        "scripts": [
            "commons/dispatcher.js",
            "commons/observable.js"
        ]
    },
    
    "baseComponent": {
        "dependencies": ["observable"],
        "scripts": "commons/component.js"
    },

    "base": {
        "scripts": [
            "commons/router.js",
            "commons/browserEvent.js",
            "commons/view.js",
            "commons/htmlElement.js",
            "commons/complexView.js",
            "commons/model.js",
            "commons/repository.js",
            "commons/stores/localstorage.js"
        ],
        "styles": "/commons/core.css",
        "dependencies": ["jQuery", "baseComponent"]
    },

    "compMap": {
        "scripts": "components/map.js"
    },

    "compCollate": {
        "dependencies": ["compMap"],
        "scripts": "components/collate.js"
    },

    "compTextbox": {
        "dependencies": ["compMap"],
        "scripts": "views/textbox.js"
    },

    "compButton": {
        "scripts": "views/button.js"
    },

    "compFilter": {
        "scripts": "components/filter.js"
    },

    "compSampleOn": {
        "scripts": "components/sampleOn.js"
    },

    "compMerge": {
        "scripts": "components/merge.js"
    },

    "compWrite": {
        "dependencies": ["compMap"],
        "scripts": "components/write.js"
    },

    "compRead": {
        "dependencies": ["compMap"],
        "scripts": "components/read.js"
    },

    "compDelete": {
        "dependencies": ["compMap"],
        "scripts": "components/delete.js"
    }
}
